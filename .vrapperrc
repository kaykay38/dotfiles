set ignorecase
set autoindent
set relativenumber
set novisualmouse
set unamedplus
set scrolloff=5
nnoremap <space>v <C-v>
nnoremap <space><CR> O<Esc>j
nnoremap <CR> o<Esc>k
nnoremap fi gg=G<C-o><C-o>
nnoremap <space>a ggVG
inoremap <A-j> <Down>
inoremap <A-k> <Up>
inoremap <A-h> <Left>
inoremap <A-l> <Right>
